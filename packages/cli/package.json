{
  "name": "docslurp",
  "version": "0.1.0",
  "description": "Turn any documentation into an MCP server with RAG capabilities",
  "type": "module",
  "bin": {
    "docslurp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "rag",
    "documentation",
    "claude",
    "ai",
    "embeddings",
    "vector-search"
  ],
  "author": "James Agudo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesagudo/docslurp"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "cheerio": "^1.0.0",
    "openai": "^4.0.0",
    "better-sqlite3": "^11.0.0",
    "sqlite-vec": "^0.1.6",
    "ora": "^8.0.0",
    "chalk": "^5.0.0",
    "node-fetch": "^3.3.0",
    "@mendable/firecrawl-js": "^1.0.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
